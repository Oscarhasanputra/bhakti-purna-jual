var LoginService = (function () {
    // Resolve HTTP using the constructor
    function LoginService(http, global) {
        this.http = http;
        this.global = global;
    }
    LoginService.prototype.login = function (body) {
        var bodyString = JSON.stringify(body); // Stringify payload
        var headers = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["Headers"]({ 'Content-Type': 'application/json' });
        var options = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["RequestOptions"]({ headers: headers });
        return this.http.post(this.global.getGlobalUrl() + '/login', body, options) // ...using post request
            .toPromise()
            .then(this.setSessionUser)
            .catch(this.handleError);
    };
    LoginService.prototype.getBassDetail = function (body) {
        var bodyString = JSON.stringify({ kode_bass: body }); // Stringify payload
        var token = JSON.parse(sessionStorage.getItem('mAuth')).Token;
        var headers = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["Headers"]({ 'Content-Type': 'application/json', 'x-access-token': token });
        var options = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["RequestOptions"]({ headers: headers });
        return this.http.post(this.global.getGlobalUrl() + '/get_bassdetail', bodyString, options) // ...using post request
            .toPromise()
            .then(this.setSessionBass)
            .catch(this.handleError);
    };
    LoginService.prototype.getRoleDetail = function (body) {
        var bodyString = JSON.stringify({ kode_role: body }); // Stringify payload
        var token = JSON.parse(sessionStorage.getItem('mAuth')).Token;
        var headers = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["Headers"]({ 'Content-Type': 'application/json', 'x-access-token': token });
        var options = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["RequestOptions"]({ headers: headers });
        return this.http.post(this.global.getGlobalUrl() + '/get_roledetail', bodyString, options) // ...using post request
            .toPromise()
            .then(this.setSessionRole)
            .catch(this.handleError);
    };
    LoginService.prototype.getSystemParameter = function () {
        // let bodyString = JSON.stringify({ kode_role: body }); // Stringify payload
        var token = JSON.parse(sessionStorage.getItem('mAuth')).Token;
        var headers = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["Headers"]({ 'Content-Type': 'application/json', 'x-access-token': token });
        var options = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["RequestOptions"]({ headers: headers });
        return this.http.get(this.global.getGlobalUrl() + '/get_systemparam', options) // ...using get request
            .toPromise()
            .then(this.setSessionParameter)
            .catch(this.handleError);
    };
    LoginService.prototype.setSessionUser = function (res) {
        var body = res.json();
        // save to session storage
        try {
            var auth = {
                Kode_Bass: body[0].KODE_BASS,
                Kode_Karyawan: body[0].USERNAME,
                Role: body[0].KODE_ROLE,
                Type: body[0].TYPE === 'C' ? 'Cabang' : 'Bass',
                Token: body[0].TOKEN
            };
            sessionStorage.setItem('mAuth', JSON.stringify(auth));
        }
        catch (error) {
            console.log(error);
        }
    };
    LoginService.prototype.setSessionBass = function (res) {
        var body = res.json();
        // save to session storage
        try {
            var bass = {
                Nama_Bass: body[0].NAMA_BASS,
                Alamat_Bass: body[0].ALAMAT_BASS,
                No_Telp: body[0].NO_TELP,
                Kota: body[0].KOTA,
                Contact_Person: body[0].CONTACT_PERSON,
                Email: body[0].EMAIL
            };
            sessionStorage.setItem('mBass', JSON.stringify(bass));
        }
        catch (error) {
            console.log(error);
        }
    };
    LoginService.prototype.setSessionRole = function (res) {
        var body = res.json();
        // save to session storage
        try {
            var role_1 = [];
            body.forEach(function (element) {
                role_1.push({
                    App_Code: element.KODE_APPLICATION,
                    Akses: element.HAK_AKSES,
                    Insert: element.HAK_INSERT,
                    Edit: element.HAK_EDIT,
                    Delete: element.HAK_DELETE
                });
            });
            sessionStorage.setItem('mRole', JSON.stringify(role_1));
        }
        catch (error) {
            console.log(error);
        }
    };
    LoginService.prototype.setSessionParameter = function (res) {
        var body = res.json();
        // save to session storage
        try {
            var param = {
                Web_Title: body[0].WEB_TITLE,
                Report_Server_Path: body[0].REPORT_SERVER_PATH,
                Expired_PO_Date: body[0].EXPIRED_PO_DATE,
                Bass_Pusat: body[0].BASS_PUSAT,
                Email_SMTP: body[0].EMAIL_SMTP,
                Email_Port: body[0].EMAIL_PORT,
                Email_Username: body[0].EMAIL_USERNAME,
                Email_Password: body[0].EMAIL_PASSWORD
            };
            sessionStorage.setItem('mParameter', JSON.stringify(param));
        }
        catch (error) {
            console.log(error);
        }
    };
    LoginService.prototype.handleError = function (error) {
        return Promise.reject(error);
    };
    LoginService = __decorate([
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Injectable"])(), 
        __metadata('design:paramtypes', [__WEBPACK_IMPORTED_MODULE_1__angular_http__["Http"], __WEBPACK_IMPORTED_MODULE_2__global_state__["a" /* GlobalState */]])
    ], LoginService);
    return LoginService;
}());


