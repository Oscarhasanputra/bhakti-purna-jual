                                        // console.log(data.result)
                                    }, function (err) {
                                        if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                            alert("Your Token has expired, please login again !");
                                            sessionStorage.clear();
                                            _this.router.navigate(['/login']);
                                        }
                                        else {
                                            alert(err._body.data);
                                        }
                                    });
                                }
                            }
