var Debounce = (function () {
    function Debounce(elementRef, model) {
        this.elementRef = elementRef;
        this.model = model;
        this.delay = 700;
        this.func = new __WEBPACK_IMPORTED_MODULE_0__angular_core__["EventEmitter"]();
    }
    Debounce.prototype.ngOnInit = function () {
        var _this = this;
        var eventStream = __WEBPACK_IMPORTED_MODULE_1_rxjs__["Observable"].fromEvent(this.elementRef.nativeElement, 'keyup')
            .map(function () { return _this.model.value; })
            .debounceTime(this.delay);
        eventStream.subscribe(function (input) { return _this.func.emit(input); });
    };
    __decorate([
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Input"])(),
        __metadata('design:type', Number)
    ], Debounce.prototype, "delay", void 0);
    __decorate([
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Output"])(),
        __metadata('design:type', __WEBPACK_IMPORTED_MODULE_0__angular_core__["EventEmitter"])
    ], Debounce.prototype, "func", void 0);
    Debounce = __decorate([
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Directive"])({ selector: '[debounce]' }),
        __metadata('design:paramtypes', [__WEBPACK_IMPORTED_MODULE_0__angular_core__["ElementRef"], __WEBPACK_IMPORTED_MODULE_2__angular_forms__["NgModel"]])
    ], Debounce);
    return Debounce;
