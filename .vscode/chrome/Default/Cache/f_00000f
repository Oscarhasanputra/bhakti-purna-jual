        this.KodeBass = JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass;
        this.busyLoaderEvent.template = "<div style=\"margin-top:150px; margin-left:550px; position: fixed; z-index:1000; text-align:center; font-size: 24px; \">\n                                      <i class=\"fa fa-spinner fa-spin\" style=\"font-size:36px;\"></i>\n                                      {{message}}\n                                      </div>";
    }
    ReviewClaim.prototype.ngDoCheck = function () {
        if (this.NoClaim !== this.oldNoClaim) {
            this.services = [];
            this.oldNoClaim = this.NoClaim;
            this.loadData(this.NoClaim);
        }
    };
    ReviewClaim.prototype.loadData = function (noClaim) {
        var _this = this;
        this.busyLoaderEvent.busy = this.service.getClaimService(noClaim).then(function (data) {
            _this.services = data;
        }, function (err) {
            if (err._body.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body);
            }
        });
    };
    ReviewClaim.prototype.cancel = function () {
        this.viewState.emit(false);
    };
    ReviewClaim.prototype.reviewService = function (service) {
        var _this = this;
        // console.log(service);
        if (service.ISVALID == '0' && service.REASON == '') {
            alert('Catatan harap diisi!');
        }
        else {
            this.confirmationService.confirm({
                header: 'Confirmation',
                message: 'Simpan review claim service?',
                accept: function () {
                    if (service.ISVALID == '1') {
                        service.REASON = '';
                    }
                    //Actual logic to perform a confirmation
                    _this.busyLoaderEvent.busy = _this.service.insertReviewClaim(_this.NoClaim, service.KODE_SERVICE, Number(service.ISVALID), service.REASON, JSON.parse(sessionStorage.getItem('mAuth')).Kode_Karyawan, _this.KodeBass, new Date()).then(function (data) {
                        alert(data);
                        _this.loadData(_this.NoClaim);
                    }, function (err) {
                        if (err._body.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                            alert("Your Token has expired, please login again !");
                            sessionStorage.clear();
                            _this.router.navigate(['/login']);
                        }
                        else {
                            alert(err._body);
                        }
                    });
                }
            });
        }
    };
    ReviewClaim.prototype.changeBgColor = function (rowData, rowIndex) {
        return rowData.STATUS === '1' ? 'row-backgroundcolor' : '';
    };
