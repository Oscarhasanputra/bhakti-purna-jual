var ClaimList = (function () {
    function ClaimList(service, router, confirmationService) {
        var _this = this;
        this.service = service;
        this.router = router;
        this.confirmationService = confirmationService;
        this.busyLoaderEvent = Object.assign({}, __WEBPACK_IMPORTED_MODULE_3_angular2_busy__["BUSY_CONFIG_DEFAULTS"]);
        this.appCode = "APPL00008";
        this.display = false;
        this.busyLoaderEvent.template = "<div style=\"margin-top:150px; margin-left:450px; position: fixed; z-index:1000; text-align:center; font-size: 24px; \">\n                                      <i class=\"fa fa-spinner fa-spin\" style=\"font-size:36px;\"></i>\n                                      {{message}}\n                                      </div>";
        var today = new Date();
        var month = today.getMonth();
        var year = today.getFullYear();
        var prevMonth = (month === 0) ? 11 : month - 1;
        var prevYear = (prevMonth === 11) ? year - 1 : year;
        this.dateFr = new Date();
        this.dateFr.setMonth(prevMonth);
        this.dateFr.setFullYear(prevYear);
        this.dateTo = new Date();
        this.searchClaim = "";
        this.selectedClaim = "";
        this.selectedDate = new Date();
        this.statusClaim = [];
        this.statusClaim.push({ value: '0', desc: 'Belum di Review' });
        this.statusClaim.push({ value: '1', desc: 'Unpaid' });
        this.statusClaim.push({ value: '2', desc: 'Paid' });
        this.statusClaim.push({ value: '3', desc: 'Cannot be Paid' });
        this.statusClaim.push({ value: '', desc: 'All' });
        this.selectedStatus = this.statusClaim[0].value;
        this.HakAkses = JSON.parse(sessionStorage.getItem('mRole')).filter(function (data) { return data.App_Code == _this.appCode; })[0];
        if (this.HakAkses.Akses) {
            this.loadData();
        }
        else {
            alert('Anda tidak berhak mengakses halaman ini!');
            this.router.navigate(['/pages/home']);
        }
    }
    ClaimList.prototype.handleModal = function (modalData) {
        this.display = modalData;
    };
    ClaimList.prototype.loadData = function () {
        var _this = this;
        var kodebass = JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass;
        if (JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass == "B061") {
            kodebass = "PUSAT";
        }
        this.busyLoaderEvent.busy = this.service.getDataClaimList(kodebass, this.searchClaim, this.dateFr.toISOString().substring(0, 10), this.dateTo.toISOString().substring(0, 10), this.selectedStatus)
            .then(function (data) {
            _this.claims = data;
        }, function (err) {
            if (err._body.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body);
            }
        });
    };
    ClaimList.prototype.deleteClaim = function (claim) {
        var _this = this;
        this.confirmationService.confirm({
            header: "Confirmation",
            message: "Hapus " + claim.KODE_CLAIM + "?",
            accept: function () {
                if (claim.STATUS == '1' || claim.STATUS == '2' || claim.STATUS == '3') {
                    alert("Claim ini sudah di review");
                }
                else {
                    _this.busyLoaderEvent.busy = _this.service.deleteDataClaimList(claim.KODE_CLAIM).then(function (data) {
                        alert(data);
                        _this.loadData();
                    }, function (err) {
                        if (err._body.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                            alert("Your Token has expired, please login again !");
                            sessionStorage.clear();
                            _this.router.navigate(['/login']);
                        }
                        else {
                            alert(err._body);
                        }
                    });
                }
            }
        });
    };
    ClaimList.prototype.editClaim = function (claim) {
        this.display = true;
        this.selectedClaim = claim.KODE_CLAIM;
        this.selectedDate = claim.TANGGAL;
    };
    ClaimList = __decorate([
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Component"])({
