    FinishingServiceRequestComponent.prototype.validation = function (data) {
        var aTanggalSelesai = new Date(data.tanggalSelesai);
        var aTanggal = new Date(data.tanggal);
        var aTanggalKembali = new Date(data.tanggalKembali);
        if (data.tanggalSelesai == "") {
            alert("Tanggal selesai harus diisi");
            return false;
        }
        else if (data.nomorNota == "") {
            alert("Nomor Nota harus diisi");
            return false;
        }
        else if (data.kodeTeknisi == "") {
            alert("Teknisi harus diisi");
            return false;
        }
        else if (aTanggalSelesai < aTanggal) {
            alert("Tanggal Selesai harus lebih besar daripada Tanggal Service");
            return false;
        }
        else {
            if (data.tanggalKembali != "") {
                if (aTanggalKembali < aTanggalSelesai) {
                    alert("Tanggal Kembali harus lebih besar daripada Tanggal Selesai");
                    return false;
                }
                if (this.data.dataDetail.length > 0) {
                    for (var i = 0; i < this.data.dataDetail.length; i++) {
                        if (this.data.dataDetail[i].QTY <= 0) {
                            alert("Qty pada kode part : " + this.data.dataDetail[i].KD_SPAREPART + ", harus lebih besar dari 0");
                            return false;
                        }
                        else {
                            // belum berfungsi cek stok
                            // if (this.data.dataDetail[i].QTY > stok) {
                            //     alert("Quantity Melebihi Stock untuk part "+ this.data.dataDetail[i].KD_SParepart + "dan invoice "+ this.data.dataDetail[i].NO_INVOICE)
                            // }
                            return true;
                        }
                    }
                }
                else {
                    return true;
                }
            }
        }
    };
    FinishingServiceRequestComponent.prototype.cekTeknisi = function (kodeTeknisi) {
        var _this = this;
        return this.service.getTeknisi(this.gloKodeBass, kodeTeknisi, 'A').then(function (data) {
            if (data.length > 0) {
                return true;
            }
            else {
                alert("Teknisi tidak ada dalam database");
                return false;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            return false;
        });
    };
    FinishingServiceRequestComponent.prototype.newRequest = function () {
        this.router.navigate(['pages/service/servicerequest']);
    };
    FinishingServiceRequestComponent.prototype.reject = function (data) {
        var _this = this;
        if (data !== "") {
            this.service.reject({ kodeService: this.data.kodeService, status: "Rejected", rejectionComment: data }).then(function (data) {
                // console.log(data)
                alert("Sukses menyimpan data");
                _this.display = false;
                _this.Disable_Enable_Detail(false, _this.data);
                _this.flagRejected = true;
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
    };
    FinishingServiceRequestComponent.prototype.showDialog = function () {
        this.display = true;
    };
    FinishingServiceRequestComponent.prototype.cancel = function () {
        this.display = false;
    };
    FinishingServiceRequestComponent.prototype.print = function () {
        if (this.data.kodeService !== "") {
            var printContents = void 0, popupWin = void 0;
            printContents = document.getElementById('print-section').innerHTML;
            popupWin = window.open('', '_blank', 'top=0,left=0,height=100%,width=auto');
            popupWin.document.open();
            popupWin.document.write("\n                    <html>\n                    <head>\n                        <title>Service Reuest</title>\n                        <style>\n\n                        .mytable { \n                            border-collapse: collapse; \n                            margin-top:10px;\n                            margin-bottom:40px;\n                            table-layout: fixed;\n                            width: 100%;\n                        }\n                        /* Zebra striping */\n                        .mytable tr:nth-of-type(odd) { \n                            background: #eee; \n                            }\n                        .mytable th { \n                            background: #3498db; \n                            color: white; \n                            }\n                        .mytable td, th { \n                            padding: 7px; \n                            border: 1px solid #ccc; \n                            text-align: center;\n                            font-size: 10px;\n                            }\n                        .mytable .nourut {\n                            width:30px;\n                        }\n\n                        .kodeclaim {\n                            font-weight: bold;\n                            font-size: 14px;\n                        }\n\n                        .div-jarak {\n                            margin:10px 0px 10px 0px;\n                        }\n                        </style>\n                    </head>\n                <body onload=\"window.print();window.close()\">\n                    " + printContents + "\n                </body>\n                    </html>");
            popupWin.document.close();
        }
    };
