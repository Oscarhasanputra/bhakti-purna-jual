        this.flaghidenMain = false;
    };
    // ambil data part
    FinishingServiceRequestAllComponent.prototype.onRowSelectedLookupPart = function (data) {
        // console.log("onRowSelectedLookUpPart : " + data)
        for (var i = 0; i < data.length; i++) {
            var flagEqual = false;
            for (var ii = 0; ii < this.data.dataDetail.length; ii++) {
                // stok2an blom beres
                this.getStokInvoiceSelectByKodePartAndInvoice(this.gloKodeBass, this.data.dataDetail[ii].KD_SPAREPART, this.data.dataDetail[ii].NO_INVOICE);
                if (data[i].KD_SPAREPART == this.data.dataDetail[ii].KD_SPAREPART) {
                    flagEqual = true;
                    break;
                }
            }
            if (!flagEqual) {
                this.data.dataDetail.push(data[i]);
            }
        }
        this.flaghidenMain = false;
        this.calculate();
    };
    FinishingServiceRequestAllComponent.prototype.onRowSelectedFaktur = function (data) {
        // console.log("onRowSelectedFaktur : " + data)
        for (var i = 0; i < data.length; i++) {
            var flagEqual = false;
            for (var ii = 0; ii < this.data.dataDetail.length; ii++) {
                // stok2an blom beres
                this.getStokInvoiceSelectByKodePartAndInvoice(this.gloKodeBass, this.data.dataDetail[ii].KD_SPAREPART, this.data.dataDetail[ii].NO_INVOICE);
                if (data[i].KD_SPAREPART == this.data.dataDetail[ii].KD_SPAREPART) {
                    flagEqual = true;
                    break;
                }
            }
            if (!flagEqual) {
                this.data.dataDetail.push(data[i]);
            }
        }
        this.flaghidenMain = false;
        this.calculate();
        alert(this.flagButtonInsert);
    };
    FinishingServiceRequestAllComponent.prototype.onChangeComboBiayaTransport = function () {
        this.calculate();
    };
    FinishingServiceRequestAllComponent.prototype.onChangeQtySparepart = function () {
        this.calculate();
    };
    FinishingServiceRequestAllComponent.prototype.onDeletedDetail = function (index) {
        this.data.dataDetail.splice(index, 1);
        this.calculate();
    };
    // event onchange combobox status barang
    FinishingServiceRequestAllComponent.prototype.onChangeStatusBarang = function (status) {
