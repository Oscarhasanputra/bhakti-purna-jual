var PaidClaimServices = (function () {
    function PaidClaimServices(service, router, confirmationService, global) {
        var _this = this;
        this.service = service;
        this.router = router;
        this.confirmationService = confirmationService;
        this.global = global;
        this.busyLoaderEvent = Object.assign({}, __WEBPACK_IMPORTED_MODULE_3_angular2_busy__["BUSY_CONFIG_DEFAULTS"]);
        this.appCode = "APPL00062";
        this.selectedStatus = "";
        this.kodeBassParam = "";
        this.busyLoaderEvent.template = "<div style=\"margin-top:150px; margin-left:450px; position: fixed; z-index:1000; text-align:center; font-size: 24px; \">\n                                      <i class=\"fa fa-spinner fa-spin\" style=\"font-size:36px;\"></i>\n                                      {{message}}\n                                      </div>";
        var today = new Date();
        var month = today.getMonth();
        var year = today.getFullYear();
        var prevMonth = (month === 0) ? 11 : month - 1;
        var prevYear = (prevMonth === 11) ? year - 1 : year;
        this.dateFr = new Date();
        this.dateFr.setMonth(prevMonth);
        this.dateFr.setFullYear(prevYear);
        this.dateTo = new Date();
        this.statusClaim = [{ value: '0', desc: 'Belum di Review' },
            { value: '1', desc: 'Unpaid' },
            { value: '2', desc: 'Paid' },
            { value: '', desc: 'All' }];
        this.selectedStatus = this.statusClaim[0].value;
        this.HakAkses = JSON.parse(sessionStorage.getItem('mRole')).filter(function (data) { return data.App_Code == _this.appCode; })[0];
        if (this.HakAkses.Akses) {
            if (JSON.parse(sessionStorage.getItem('mAuth')).Type == "Cabang" ||
                JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass == JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat) {
                this.loadData();
            }
            else {
                alert('Anda tidak berhak mengakses halaman ini!');
                this.router.navigate(['/pages/home']);
            }
        }
        else {
            alert('Anda tidak berhak mengakses halaman ini!');
            this.router.navigate(['/pages/home']);
        }
    }
    PaidClaimServices.prototype.handleModal = function (childData) {
        this.kodeBassParam = childData;
    };
    PaidClaimServices.prototype.loadData = function () {
        var _this = this;
        this.busyLoaderEvent.busy = this.service.getReviewClaimList(JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass, this.kodeBassParam, this.dateFr.toISOString().substring(0, 10), this.dateTo.toISOString().substring(0, 10), this.selectedStatus)
            .then(function (data) {
            _this.claims = data;
        }, function (err) {
            if (err._body.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body);
            }
        });
    };
    PaidClaimServices.prototype.paidClaim = function (claim) {
        var _this = this;
        this.confirmationService.confirm({
            header: 'Confirmation',
            message: 'Pay claim service?',
            accept: function () {
                //Actual logic to perform a confirmation
                _this.busyLoaderEvent.busy = _this.service.insertPaidClaim(claim.KODE_CLAIM, JSON.parse(sessionStorage.getItem('mAuth')).Kode_Karyawan, JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass, new Date())
                    .then(function (data) {
                    alert(data);
                    _this.loadData();
                }, function (err) {
                    if (err._body.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                        alert("Your Token has expired, please login again !");
                        sessionStorage.clear();
                        _this.router.navigate(['/login']);
                    }
                    else {
                        alert(err._body);
                    }
                });
            }
        });
    };
    PaidClaimServices.prototype.changeBgColor = function (rowData, rowIndex) {
        switch (rowData.STATUS) {
            case 1: {
                return 'row-backgroundcolor-paid';
            }
            case 2: {
                return 'row-backgroundcolor-unpaid';
            }
            case 3: {
                return 'row-backgroundcolor-cannot';
            }
            default: {
                return '';
            }
        }
    };
