var ReviewClaim = (function () {
    function ReviewClaim(service, router, confirmationService) {
        this.service = service;
        this.router = router;
        this.confirmationService = confirmationService;
        this.viewState = new __WEBPACK_IMPORTED_MODULE_0__angular_core__["EventEmitter"]();
        this.busyLoaderEvent = Object.assign({}, __WEBPACK_IMPORTED_MODULE_3_angular2_busy__["BUSY_CONFIG_DEFAULTS"]);
