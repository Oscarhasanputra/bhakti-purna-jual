/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "a", function() { return FinishingServiceRequestAllComponent; });




var FinishingServiceRequestAllComponent = (function () {
    function FinishingServiceRequestAllComponent(router, actroute, servicerequest, service) {
        this.router = router;
        this.actroute = actroute;
        this.servicerequest = servicerequest;
        this.service = service;
        this.kodePart = "";
        this.appCode = "APPL00013";
        this.saveTextButton = "Save";
        this.today = new Date();
        this.flagButtonInsert = false;
        this.flagButtonUpdate = false;
        this.flagButtonClose = false;
        this.flagButtonReject = false;
        this.flagButtonReOpen = false;
        this.flagEdit = false;
        this.flaghidenMain = false;
        this.flagRejected = false;
        this.statusReviewNotValid = false;
        this.flaghidenInputCustomer = false;
        this.flagAssignToBass = false;
        // flag for property
        this.flagTanggal = true;
        this.flagTanggalSelesaiTextbox = true;
        this.flagTanggalKembaliTextbox = true;
        this.flagDiambilOlehCustomerTextbox = true;
        this.flagPerbaikanCombo = true;
        this.flagPenyebabCombo = true;
        this.flagTeknisi = true;
        this.flagBiayaTransportCombo = true;
        this.flagSparepartsQty = true;
        this.flagNomorInoviceTextbox = true;
        this.flagNomorInvoiceButton = true;
        this.flagKodeSparepartTextbox = true;
        this.flagNomorSeriTextbox = true;
