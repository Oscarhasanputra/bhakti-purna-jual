    FinishingServiceRequestComponent.prototype.close = function () {
        var _this = this;
        if (this.data.kodeService !== "") {
            this.service.updatestatus(this.data.kodeService, "Closed").then(function (data) {
                // jika ada di table review dan statusnya 0 maka delete dari table claim service dan buat claim servicenya ''
