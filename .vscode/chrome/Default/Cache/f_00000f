                _this.partList = data;
                // console.log("jumlah row part : ", this.partList.length)
                if (_this.partList.length == 1) {
                    var event_1 = { data: data[0] };
                    _this.onRowSelected.emit(event_1);
                }
                else if (_this.partList.length > 1) {
                    _this.flagHidden = true;
                    _this.onHiddenMain.emit(true);
                }
                else {
                    alert("Part Tidak ada dalam database");
                }
