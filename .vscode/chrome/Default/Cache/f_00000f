        this.flaghidenMain = false;
    };
    ServiceRequestComponent.prototype.onCancelLookupCustomer = function (flag) {
        this.flaghidenMain = false;
    };
    // event - event customer list
    // event - event barang list
    ServiceRequestComponent.prototype.onRowSelectedLookupBarang = function (event) {
