var BarangListComponent = (function () {
    function BarangListComponent(router, actroute, service) {
        this.router = router;
        this.actroute = actroute;
        this.service = service;
        this.flagHidden = false;
        this.barangfilter = { kodeBarang: "", merk: "", jenis: "" };
        this.busyloadevent = Object.assign({}, __WEBPACK_IMPORTED_MODULE_2_angular2_busy__["BUSY_CONFIG_DEFAULTS"]);
        this.onHiddenMain = new __WEBPACK_IMPORTED_MODULE_0__angular_core__["EventEmitter"]();
        this.onCancel = new __WEBPACK_IMPORTED_MODULE_0__angular_core__["EventEmitter"]();
        this.onRowSelected = new __WEBPACK_IMPORTED_MODULE_0__angular_core__["EventEmitter"]();
        this.busyloadevent.template = '<div style="margin-top: 10px; text-align: center; font-size: 25px; font-weight: 700;"><i class="fa fa-spinner fa-spin" style="font-size:34px"></i>{{message}}</div>';
    }
    BarangListComponent.prototype.ngOnInit = function () {
        this.gloKodeBass = JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass;
        this.gloNamaBass = JSON.parse(sessionStorage.getItem('mBass')).Nama_Bass;
        this.gloUsername = JSON.parse(sessionStorage.getItem('mAuth')).Kode_Karyawan;
    };
    BarangListComponent.prototype.buttonBarangBrowseClicked = function () {
        this.getMerk();
        this.getJenis();
        this.getBarang("", "", "");
        this.flagHidden = true;
        this.onHiddenMain.emit(true);
    };
    // *
    BarangListComponent.prototype.searchBarangClicked = function (kodeBarang, merk, jenis) {
        if (merk == "ALL" && jenis == "ALL") {
            this.getBarang(kodeBarang, "", "");
        }
        else if (merk == "ALL" && jenis !== "ALL") {
            this.getBarang(kodeBarang, "", jenis);
        }
        else if (merk !== "ALL" && jenis == "ALL") {
            this.getBarang(kodeBarang, merk, "");
        }
        else {
            this.getBarang(kodeBarang, merk, jenis);
        }
    };
    BarangListComponent.prototype.onRowSelectBarang = function (event) {
        this.flagHidden = false;
        this.onRowSelected.emit(event);
    };
    BarangListComponent.prototype.onCancelBarangBrowse = function () {
        this.flagHidden = false;
        this.onCancel.emit(false);
    };
    // search debounce barang
    BarangListComponent.prototype.debounceSearchBarang = function (kodeBarang) {
        this.barangfilter['kodeBarang'] = kodeBarang;
        this.getMerk();
        this.getJenis();
        this.searchBarangClicked(kodeBarang, "", "");
        this.flagHidden = true;
        this.onHiddenMain.emit(true);
    };
    BarangListComponent.prototype.searchBarangEventOnBlur = function (filter) {
        var _this = this;
        if (filter !== '') {
            this.barangfilter['kodeBarang'] = filter;
            this.busyloadevent.busy = this.service.getBarang(filter, '', '').then(function (data) {
                _this.barangList = data;
                if (_this.barangList.length == 1) {
                    var event_1 = { data: data[0] };
                    _this.onRowSelected.emit(event_1);
                }
                else if (_this.barangList.length > 1) {
                    _this.flagHidden = true;
                    _this.onHiddenMain.emit(true);
                }
                else {
                    alert("Barang Tidak ada dalam database");
                }
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
    };
    BarangListComponent.prototype.getMerk = function () {
        var _this = this;
        this.busyloadevent.busy = this.service.getMerk().then(function (data) {
            _this.merkList = data;
            _this.barangfilter['merk'] = _this.merkList[0].KODE_MEREK;
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    BarangListComponent.prototype.getJenis = function () {
        var _this = this;
        this.busyloadevent.busy = this.service.getJenis().then(function (data) {
            _this.jenisList = data;
            _this.barangfilter['jenis'] = _this.jenisList[0].KODE_JENIS;
