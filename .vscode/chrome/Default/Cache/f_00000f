    };
    // fungsi buat panggil api
    FinishingServiceRequestAllComponent.prototype.getDetailServiceRequest = function (kodeService) {
        var _this = this;
        this.service.getDetailServiceRequest(kodeService).then(function (data) {
            _this.data.dataDetail = data;
            // console.log("get detail : " + this.data.dataDetail)
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestAllComponent.prototype.getReviewClaimService = function (kodeService) {
        var _this = this;
        this.service.getReviewClaimService(kodeService).then(function (data) {
            // console.log("get review claim service : " + data)
            /*
                belum berfungsi
                jika row nya > 0
                jika row = 0 maka status review not valid
                jika status review not valid = true mka disable enable true dan sebaliknya
            */
            if (data.length > 0) {
                if (data[0].ISVALID = 0) {
                    _this.statusReviewNotValid = true;
                }
            }
            if (_this.statusReviewNotValid) {
                _this.Disable_Enable_Detail(true, _this.data);
            }
            else {
                _this.Disable_Enable_Detail(false, _this.data);
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestAllComponent.prototype.getStokInvoiceSelectByKodePartAndInvoice = function (kodeBass, kodePart, kodeInvoice) {
        var _this = this;
        this.service.getStokInvoiceSelectByKodePartAndInvoice(kodeBass, kodePart, kodeInvoice).then(function (data) {
            // console.log("stok :" + data)
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestAllComponent.prototype.getDetailServiceRequestReceived = function (kodeService) {
        var _this = this;
        this.service.getDetailServiceRequestReceived(kodeService).then(function (data) {
            _this.detailServiceRequestReceivedList = data;
            // console.log(this.detailServiceRequestReceivedList)
            if (_this.detailServiceRequestReceivedList.length > 0) {
                _this.LblNotificationPO = false;
            }
            else {
                _this.LblNotificationPO = true;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestAllComponent.prototype.ongkosService = function (status, kodeBarang, merk, jenis) {
        var _this = this;
        if (status == "NonWarranty") {
            this.servicerequest.getHarga(kodeBarang, merk, jenis).then(function (data) {
                _this.data['biaya'] = data[0].ongkos;
                _this.calculate();
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
        else {
            this.data['biaya'] = 0;
            this.calculate();
        }
    };
    // ppn
    FinishingServiceRequestAllComponent.prototype.hitungPPN = function (hargaSukuCadang, hargaService, hargaTransport) {
        var _this = this;
        // console.log("ambil biaya ppn")
        this.service.calculatePPN(hargaSukuCadang, hargaService, hargaTransport).then(function (data) {
            _this.ppn = data[0].ppn;
            // console.log("ini ppn : " + this.ppn)
            // total biaya service
            _this.totalBiayaService = hargaSukuCadang + hargaService + hargaTransport + _this.ppn;
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            // console.log(err)
        });
    };
    // biaya transport
    FinishingServiceRequestAllComponent.prototype.biayaTransport = function () {
        var _this = this;
        // console.log("fect biaya transport")
        this.service.getBiayaTransportasi().then(function (data) {
            _this.biayaTransportList = data;
            _this.biayaTransportasi = _this.biayaTransportList[0];
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            // console.log(err)
        });
    };
    FinishingServiceRequestAllComponent.prototype.pengaduan = function (kodeBarang) {
        var _this = this;
        // console.log("fect data pengaduan")
        this.servicerequest.getKerusakan(kodeBarang).then(function (data) {
            _this.kerusakanList = data;
            if (_this.data['kodePengaduan'] == "") {
                _this.data['kodePengaduan'] = _this.kerusakanList[0].Nama_Kerusakan;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestAllComponent.prototype.perbaikan = function (kodeBarang, namaKerusakan, namaPenyebab) {
        var _this = this;
        // console.log("fect data perbaikan")
        this.service.getPerbaikan(kodeBarang, namaKerusakan, namaPenyebab).then(function (data) {
            _this.perbaikanList = data;
            // console.log(this.perbaikanList)
            if (_this.perbaikanList.length > 0) {
                _this.data['perbaikan'] = _this.perbaikanList[0].NAMA_PERBAIKAN;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestAllComponent.prototype.penyebab = function (kodeBarang, namaKerusakan) {
        var _this = this;
        // console.log("fect data penyebab")
        // console.log(kodeBarang + " " + namaKerusakan)
        this.service.getPenyebab(kodeBarang, namaKerusakan).then(function (data) {
            _this.penyebabList = data;
            if (_this.penyebabList.length > 0) {
                _this.data['penyebab'] = _this.penyebabList[0].Nama_Penyebab;
                _this.perbaikan(kodeBarang, namaKerusakan, _this.penyebabList[0].Nama_Penyebab);
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    // fungsi lain lain
    FinishingServiceRequestAllComponent.prototype.onHiddenMain = function (flag) {
        this.flaghidenMain = flag;
    };
    // fungsi menghitung biaya service
    FinishingServiceRequestAllComponent.prototype.calculate = function () {
        var totalSukuCadang = 0;
        // hitung total suku cadang
        if (this.data.dataDetail.length > 0) {
            for (var i = 0; i < this.data.dataDetail.length; i++) {
                totalSukuCadang += Number(this.data.dataDetail[i].QTY) * Number(this.data.dataDetail[i].HARGA);
            }
        }
        // cek garansi jika status waranty makan total suku cadang dan biaya service = 0
        if (this.data.statusProduk == "Warranty") {
            this.hargaTotalSukuCadang = 0;
            this.biayaService = 0;
        }
        else {
            this.hargaTotalSukuCadang = totalSukuCadang;
            this.biayaService = this.data.biaya;
        }
        this.subTotal = this.hargaTotalSukuCadang + this.biayaService + this.biayaTransportasi.BIAYA_TRANSPORTASI;
        // // hitung ppn
        this.hitungPPN(this.hargaTotalSukuCadang, this.biayaService, this.biayaTransportasi.BIAYA_TRANSPORTASI);
    };
    // crud button
    FinishingServiceRequestAllComponent.prototype.save = function () {
        var _this = this;
        // validasi
        if (this.validation(this.data)) {
            // cek teknisi di database
            this.cekTeknisi(this.data.kodeTeknisi).then(function (result) {
                if (result) {
                    // cek customer di database
                    _this.cekCustomer(_this.data.kodeCustomer).then(function (result) {
                        if (result) {
                            // cek barang di database
                            _this.cekBarang(_this.data.kodeProduk).then(function (result) {
                                if (result) {
                                    _this.data.biayaPPN = _this.ppn;
                                    _this.data.biayaTotal = _this.totalBiayaService;
                                    _this.data.biyaTransport = _this.biayaTransportasi.BIAYA_TRANSPORTASI;
                                    _this.data.kodeTransportasi = _this.biayaTransportasi.KODE_TRANS;
                                    // console.log(this.data)
                                    _this.service.saveServiceFinishingAll(_this.data).then(function (data) {
                                        _this.service.getDetailServiceRequestReceived(_this.data.kodeService).then(function (data) {
                                            if (data.length > 0) {
                                                for (var i = 0; i < data.length; i++) {
                                                    var kodeSparepart = _this.data[i].KD_SPAREPART;
                                                    var noInvoice = _this.data[i].NO_INVOICE;
                                                    var harga = _this.data[i].HARGA;
                                                    var qty = _this.data[i].QTY;
                                                    var descripsi = "Penambahan Stock karena pengeditan service request = " + kodeSparepart;
                                                    _this.service.stokInsert(_this.gloKodeBass, kodeSparepart, noInvoice, _this.today, descripsi, Number(qty) * 1, _this.data.kodeService).then(function (data) {
                                                        // console.log(data.result)
                                                    }, function (err) {
                                                        if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                                            alert("Your Token has expired, please login again !");
                                                            sessionStorage.clear();
                                                            _this.router.navigate(['/login']);
                                                        }
                                                        else {
                                                            alert(err._body.data);
                                                        }
                                                    });
                                                }
                                            }
                                        }, function (err) {
                                            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                                alert("Your Token has expired, please login again !");
                                                sessionStorage.clear();
                                                _this.router.navigate(['/login']);
                                            }
                                            else {
                                                alert(err._body.data);
                                            }
                                        });
                                        for (var i = 0; i < _this.data.dataDetail.length; i++) {
                                            if (_this.data.dataDetail[i].NO_INVOICE !== "") {
                                                var kodeSparepart = _this.data.dataDetail[i].KD_SPAREPART;
                                                var noInvoice = _this.data.dataDetail[i].NO_INVOICE;
                                                var harga = _this.data.dataDetail[i].HARGA;
                                                var qty = _this.data.dataDetail[i].QTY;
                                                var descripsi = "Pengurangan karena Finishing Service dengan Nomor Service = " + kodeSparepart;
                                                _this.service.stokInsert(_this.gloKodeBass, kodeSparepart, noInvoice, _this.today, descripsi, Number(qty) * -1, _this.data.kodeService).then(function (data) {
                                                    // console.log(data.result)
                                                }, function (err) {
                                                    if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                                        alert("Your Token has expired, please login again !");
                                                        sessionStorage.clear();
                                                        _this.router.navigate(['/login']);
                                                    }
                                                    else {
                                                        alert(err._body.data);
                                                    }
                                                });
                                            }
                                        }
                                        // jika ada di table review dan statusnya 0 maka delete dari table claim service dan buat claim servicenya ''
                                        _this.service.getReviewClaimService(_this.data.kodeService).then(function (data) {
                                            if (data.length > 0) {
                                                if (data[0].ISVALID = 0) {
                                                    _this.service.updateStatusBeforeClaimService(_this.data.kodeService).then(function (data) {
                                                        // console.log("update status before claim sevice : " + data.result)
                                                    }, function (err) {
                                                        if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                                            alert("Your Token has expired, please login again !");
                                                            sessionStorage.clear();
                                                            _this.router.navigate(['/login']);
                                                        }
                                                        else {
                                                            alert(err._body.data);
                                                        }
                                                    });
                                                }
                                            }
                                        }, function (err) {
                                            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                                alert("Your Token has expired, please login again !");
                                                sessionStorage.clear();
                                                _this.router.navigate(['/login']);
                                            }
                                            else {
                                                alert(err._body.data);
                                            }
                                        });
                                        alert(data.result);
                                        _this.Disable_Enable_Detail(false, _this.data);
                                    }, function (err) {
                                        if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                            alert("Your Token has expired, please login again !");
                                            sessionStorage.clear();
                                            _this.router.navigate(['/login']);
                                        }
                                        else {
                                            alert(err._body.data);
                                        }
                                    });
                                }
                            });
                        }
                    });
                }
            });
        }
    };
    FinishingServiceRequestAllComponent.prototype.close = function () {
        var _this = this;
        if (this.data.kodeService !== "") {
            this.service.updatestatus(this.data.kodeService, "Closed").then(function (data) {
                // jika ada di table review dan statusnya 0 maka delete dari table claim service dan buat claim servicenya ''
                _this.service.getReviewClaimService(_this.data.kodeService).then(function (data) {
                    if (data.length > 0) {
                        if (data[0].ISVALID = 0) {
                            _this.service.updateStatusBeforeClaimService(_this.data.kodeService).then(function (data) {
                                // console.log("update status before claim sevice : " + data.result)
                            }, function (err) {
                                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                    alert("Your Token has expired, please login again !");
                                    sessionStorage.clear();
                                    _this.router.navigate(['/login']);
                                }
                                else {
                                    alert(err._body.data);
                                }
                            });
                        }
                    }
                }, function (err) {
                    if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                        alert("Your Token has expired, please login again !");
                        sessionStorage.clear();
                        _this.router.navigate(['/login']);
                    }
                    else {
                        alert(err._body.data);
                    }
                });
                if (_this.data.dataDetail.length > 0) {
                    for (var i = 0; i < _this.data.dataDetail.length; i++) {
                        if (_this.data.dataDetail[i].this.data.dataDetail[i].QTY !== "") {
                            var kodeSparepart = _this.data.dataDetail[i].KD_SPAREPART;
                            var noInvoice = _this.data.dataDetail[i].NO_INVOICE;
                            var harga = _this.data.dataDetail[i].HARGA;
                            var qty = _this.data.dataDetail[i].QTY;
                            var descripsi = "Penambahan Stock karena Closing service request dengan Nomor Service : " + kodeSparepart;
                            _this.service.stokInsert(_this.gloKodeBass, kodeSparepart, noInvoice, _this.today, descripsi, Number(qty), _this.data.kodeService).then(function (data) {
                                // console.log(data.result)
                            }, function (err) {
                                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                    alert("Your Token has expired, please login again !");
                                    sessionStorage.clear();
                                    _this.router.navigate(['/login']);
                                }
                                else {
                                    alert(err._body.data);
                                }
                            });
                        }
                    }
                }
                alert("Sukses menyimpan data");
                var event = [];
                event.push(_this.data);
                _this.onRowSelectedLookupService(event);
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
    };
    FinishingServiceRequestAllComponent.prototype.reOpen = function () {
        var _this = this;
        if (this.data.kodeService !== "") {
            this.service.updatestatus(this.data.kodeService, "Opened").then(function (data) {
                // console.log("update data reopen : " + data.result)
                alert("Sukses menyimpan data");
                var event = [];
                event.push(_this.data);
                _this.onRowSelectedLookupService(event);
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
    };
    FinishingServiceRequestAllComponent.prototype.validation = function (data) {
        var aTanggalSelesai = new Date(data.tanggalSelesai);
        var aTanggal = new Date(data.tanggal);
        var aTanggalKembali = new Date(data.tanggalKembali);
        if (data.tanggalSelesai == "") {
            alert("Tanggal selesai harus diisi");
            return false;
        }
        else if (data.nomorNota == "") {
            alert("Nomor Nota harus diisi");
            return false;
        }
        else if (data.kodeTeknisi == "") {
            alert("Teknisi harus diisi");
            return false;
        }
        else if (aTanggalSelesai < aTanggal) {
            alert("Tanggal Selesai harus lebih besar daripada tanggal service");
            return false;
        }
        else if (aTanggalKembali < aTanggalSelesai) {
            alert("Tanggal Kembali harus lebih besar daripada tanggal selesai");
            return false;
        }
        else {
            if (this.data.dataDetail.length > 0) {
                for (var i = 0; i < this.data.dataDetail.length; i++) {
                    if (this.data.dataDetail[i].QTY <= 0) {
                        alert("Qty pada kode part : " + this.data.dataDetail[i].KD_SPAREPART + ", harus lebih besar dari 0");
                        return false;
                    }
                    else {
                        // belum berfungsi cek stok
                        // if (this.data.dataDetail[i].QTY > stok) {
                        //     alert("Quantity Melebihi Stock untuk part "+ this.data.dataDetail[i].KD_SParepart + "dan invoice "+ this.data.dataDetail[i].NO_INVOICE)
                        // }
                        return true;
                    }
                }
            }
            else {
                return true;
            }
        }
    };
    FinishingServiceRequestAllComponent.prototype.cekCustomer = function (kodeCustomer) {
        var _this = this;
        return this.servicerequest.getCustomer(kodeCustomer).then(function (data) {
            if (data.length > 0) {
                return true;
            }
            else {
                alert("Customer tidak ada dalam database");
                return false;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            return false;
        });
    };
    FinishingServiceRequestAllComponent.prototype.cekBarang = function (kodeBarang) {
        var _this = this;
        return this.servicerequest.getBarangByKode(kodeBarang).then(function (data) {
            if (data.length > 0) {
                return true;
            }
            else {
                alert("kode barang ini tidak ada dalam database");
                return false;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            return false;
        });
    };
    FinishingServiceRequestAllComponent.prototype.cekTeknisi = function (kodeTeknisi) {
        var _this = this;
        return this.service.getTeknisi(this.gloKodeBass, kodeTeknisi, 'A').then(function (data) {
            if (data.length > 0) {
                return true;
            }
            else {
                alert("Teknisi tidak ada dalam database");
                return false;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            return false;
        });
    };
    FinishingServiceRequestAllComponent.prototype.newRequest = function () {
        this.router.navigate(['pages/service/servicerequest']);
    };
    FinishingServiceRequestAllComponent.prototype.reject = function (data) {
        var _this = this;
        if (data !== "") {
            this.service.reject({ kodeService: this.data.kodeService, status: "Rejected", rejectionComment: data }).then(function (data) {
                // console.log(data)
                alert("Sukses menyimpan data");
                _this.display = false;
                _this.Disable_Enable_Detail(false, _this.data);
                _this.flagRejected = true;
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
    };
    FinishingServiceRequestAllComponent.prototype.showDialog = function () {
        this.display = true;
    };
    FinishingServiceRequestAllComponent.prototype.cancel = function () {
        this.display = false;
    };
    // event - event customer list
    FinishingServiceRequestAllComponent.prototype.onRowSelectedLookupCustomer = function (event) {
        this.data['kodeCustomer'] = event.data.KODE_CUSTOMER;
        this.data['namaCustomer'] = event.data.NAMA_CUSTOMER;
        this.data['alamatCustomer'] = event.data.ALAMAT_CUSTOMER;
        this.data['kotaCustomer'] = event.data.KOTA_CUSTOMER;
        this.data['hpCustomer'] = event.data.HP_CUSTOMER;
        this.data['zonaCustomer'] = event.data.NAMA_ZONA;
        this.data['telpCustomer'] = event.data.TELP_CUSTOMER;
        this.assignToBass(event.data.KODE_CUSTOMER);
        this.flaghidenMain = false;
    };
    FinishingServiceRequestAllComponent.prototype.onCancelLookupCustomer = function (flag) {
        this.flaghidenMain = false;
    };
    // event - event customer list
    // event - event customer add new
    FinishingServiceRequestAllComponent.prototype.buttonCustomerTambahClicked = function () {
        this.flaghidenMain = true;
        this.flaghidenInputCustomer = true;
    };
    FinishingServiceRequestAllComponent.prototype.onSavedCustomer = function (value) {
        this.data['kodeCustomer'] = value.kode_customer;
        this.data['namaCustomer'] = value.nama_customer;
        this.data['alamatCustomer'] = value.alamat_customer;
        this.data['kotaCustomer'] = value.kota;
        this.data['hpCustomer'] = value.nomor_hp;
        this.data['zonaCustomer'] = value.zona;
        this.data['telpCustomer'] = value.nomor_telepon;
        this.assignToBass(value.kode_customer);
        this.flaghidenMain = false;
        this.flaghidenInputCustomer = false;
    };
    FinishingServiceRequestAllComponent.prototype.onCancel = function (flag) {
        this.flaghidenMain = false;
        this.flaghidenInputCustomer = false;
    };
    // event combobox assign to bass change
    FinishingServiceRequestAllComponent.prototype.onChange = function (assignToBass) {
        for (var i = 0; i < this.assignBassList.length; i++) {
            if (this.assignBassList[i].KODE_BASS == assignToBass) {
                this.data.kodeBass = this.assignBassList[i].KODE_BASS;
                this.data.alamatAssignToBass = this.assignBassList[i].ALAMAT_BASS;
                this.data.kotaAssignToBass = this.assignBassList[i].KOTA;
                this.data.emailAssignToBass = this.assignBassList[0].EMAIL;
            }
        }
    };
    // event - event barang list
    FinishingServiceRequestAllComponent.prototype.onRowSelectedLookupBarang = function (event) {
        this.data['kodeProduk'] = event.data.Kode_Barang;
        this.data['namaProduk'] = event.data.Nama_Barang;
        this.data['merk'] = event.data.Merek;
        this.data['jenis'] = event.data.Jenis;
        this.ongkosService(this.data.statusProduk, event.data.Kode_Barang, event.data.Merek, event.data.Jenis);
        this.pengaduan(event.data.Kode_Barang);
        this.flaghidenMain = false;
    };
    FinishingServiceRequestAllComponent.prototype.onCancelLookupBarang = function (flag) {
        this.flaghidenMain = false;
    };
    // event - event barang list
    // assign to bass *
    FinishingServiceRequestAllComponent.prototype.assignToBass = function (kodeCustomer) {
        var _this = this;
        if (JSON.parse(sessionStorage.getItem('mAuth')).Type == "Cabang" ||
            JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass == JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat) {
            this.flagAssignToBass = true;
            this.servicerequest.getNearestBass(kodeCustomer, this.gloKodeBass, 1).then(function (data) {
                _this.assignBassList = data;
                if (_this.data.assignToBass == "") {
                    _this.data.assignToBass = _this.assignBassList[0].KODE_BASS;
                    _this.data.alamatAssignToBass = _this.assignBassList[0].ALAMAT_BASS;
                    _this.data.kotaAssignToBass = _this.assignBassList[0].KOTA;
                    _this.data.emailAssignToBass = _this.assignBassList[0].EMAIL;
                }
            }, function (err) {
                // console.log(err._body.data)
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
        else {
            this.flagAssignToBass = false;
            this.servicerequest.getNearestBass(kodeCustomer, this.gloKodeBass, 0).then(function (data) {
                _this.assignBassList = data;
                if (_this.data.assignToBass == "") {
                    _this.data.assignToBass = _this.assignBassList[0].KODE_BASS;
                    _this.data.alamatAssignToBass = _this.assignBassList[0].ALAMAT_BASS;
                    _this.data.kotaAssignToBass = _this.assignBassList[0].KOTA;
                    _this.data.emailAssignToBass = _this.assignBassList[0].EMAIL;
                }
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
    };
    FinishingServiceRequestAllComponent.prototype.print = function () {
        if (this.data.kodeService !== "") {
