var ServiceRequestComponent = (function () {
    function ServiceRequestComponent(router, actroute, service, datepipe) {
        this.router = router;
        this.actroute = actroute;
        this.service = service;
        this.datepipe = datepipe;
        this.busyLoaderEvent = Object.assign({}, __WEBPACK_IMPORTED_MODULE_2_angular2_busy__["BUSY_CONFIG_DEFAULTS"]);
        // testing flag lookup
        this.flaghidenMain = false;
        this.flaghidenInputCustomer = false;
        this.appCode = "APPL00011";
        this.status_brg = ['Warranty', 'NonWarranty', 'ServiceWarranty'];
        this.today = new Date();
        this.flagButtonInsert = false;
        this.flagButtonUpdate = false;
        this.flagAssignToBass = false;
        this.flagTanggalDisable = false;
        this.flagInput = true;
        this.statusReviewNotValid = false;
    }
    ServiceRequestComponent.prototype.ngOnInit = function () {
        var _this = this;
        this.gloKodeBass = JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass;
        this.gloNamaBass = JSON.parse(sessionStorage.getItem('mBass')).Nama_Bass;
        this.gloUsername = JSON.parse(sessionStorage.getItem('mAuth')).Kode_Karyawan;
        // cek hak akses
        this.hakAkses = JSON.parse(sessionStorage.getItem('mRole')).filter(function (data) { return data.App_Code == _this.appCode; })[0];
        if (this.hakAkses.Akses) {
            // validasi tombol insert dan edit
            if (this.hakAkses.Insert) {
                this.flagButtonInsert = true;
            }
            if (this.hakAkses.Edit) {
                this.flagButtonUpdate = true;
            }
            // this.today.setMinutes(2)
            this.newData();
        }
        else {
            alert('Anda tidak berhak mengakses halaman ini!');
            this.router.navigate(['/pages/home']);
        }
    };
    ServiceRequestComponent.prototype.newData = function () {
        this.today = new Date();
        this.data = {
            kodeService: "",
            tanggal: this.today,
            jamMasuk: this.datepipe.transform(this.today, 'HH:mm'),
            kodeCustomer: "",
            namaCustomer: "",
            alamatCustomer: "",
            telpCustomer: "",
            hpCustomer: "",
            kotaCustomer: "",
            zonaCustomer: "",
            kodeProduk: "",
            namaProduk: "",
            merk: "",
            jenis: "",
            nomorSeri: "",
            tanggalBeli: "",
            kodeBass: this.gloKodeBass,
            jenisService: "Internal",
            statusProduk: this.status_brg[0],
            kodePengaduan: "",
            kelengkapan: "",
            catatan: "",
            biaya: "",
            status: "New",
            inputtedby: this.gloUsername,
            InputtedByBass: this.gloKodeBass,
            inputtedDate: this.today,
            nomorNota: "",
            assignToBass: "",
            alamatAssignToBass: "",
            kotaAssignToBass: "",
            emailAssignToBass: ""
        };
        this.flagTanggalDisable = false;
        this.assignToBass("");
    };
    // assign to bass *
    ServiceRequestComponent.prototype.assignToBass = function (kodeCustomer) {
        var _this = this;
        if (JSON.parse(sessionStorage.getItem('mAuth')).Type == "Cabang" ||
            JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass == JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat) {
            this.flagAssignToBass = true;
            this.service.getNearestBass(kodeCustomer, this.gloKodeBass, 1).then(function (data) {
                _this.assignBassList = data;
                if (_this.data.assignToBass == "") {
                    _this.data.assignToBass = _this.assignBassList[0].KODE_BASS;
                    _this.data.alamatAssignToBass = _this.assignBassList[0].ALAMAT_BASS;
                    _this.data.kotaAssignToBass = _this.assignBassList[0].KOTA;
                    _this.data.emailAssignToBass = _this.assignBassList[0].EMAIL;
                }
            }, function (err) {
                // console.log(err._body.data)
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
        else {
            this.flagAssignToBass = false;
            this.service.getNearestBass(kodeCustomer, this.gloKodeBass, 0).then(function (data) {
                _this.assignBassList = data;
                if (_this.data.assignToBass == "") {
                    _this.data.assignToBass = _this.assignBassList[0].KODE_BASS;
                    _this.data.alamatAssignToBass = _this.assignBassList[0].ALAMAT_BASS;
                    _this.data.kotaAssignToBass = _this.assignBassList[0].KOTA;
                    _this.data.emailAssignToBass = _this.assignBassList[0].EMAIL;
                }
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
    };
    // event combobox assign to bass change
    ServiceRequestComponent.prototype.onChange = function (assignToBass) {
        for (var i = 0; i < this.assignBassList.length; i++) {
            if (this.assignBassList[i].KODE_BASS == assignToBass) {
                this.data.kodeBass = this.assignBassList[i].KODE_BASS;
                this.data.alamatAssignToBass = this.assignBassList[i].ALAMAT_BASS;
                this.data.kotaAssignToBass = this.assignBassList[i].KOTA;
                this.data.emailAssignToBass = this.assignBassList[0].EMAIL;
            }
        }
    };
    // Pengaduan *
    ServiceRequestComponent.prototype.pengaduan = function (kodeBarang) {
        var _this = this;
        this.service.getKerusakan(kodeBarang).then(function (data) {
            _this.kerusakanList = data;
            if (_this.data['kodePengaduan'] == "") {
                _this.data['kodePengaduan'] = _this.kerusakanList[0].Nama_Kerusakan;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    ServiceRequestComponent.prototype.onChangeStatusBarang = function (status) {
        this.ongkosService(status, this.data.kodeBarang, this.data.merk, this.data.jenis);
    };
    ServiceRequestComponent.prototype.ongkosService = function (status, kodeBarang, merk, jenis) {
        var _this = this;
        if (status == "NonWarranty") {
            this.service.getHarga(kodeBarang, merk, jenis).then(function (data) {
                _this.data['biaya'] = data[0].ongkos;
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
        else {
            this.data['biaya'] = 0;
        }
    };
    ServiceRequestComponent.prototype.Disable_Enable_Detail = function (flag) {
        var _this = this;
        this.hakAkses = JSON.parse(sessionStorage.getItem('mRole')).filter(function (data) { return data.App_Code == _this.appCode; })[0];
        if (!this.hakAkses.Insert) {
            flag = false;
        }
        this.flagInput = flag;
    };
    ServiceRequestComponent.prototype.validation = function (data) {
        if (data.kodeCustomer == "") {
            alert("Kode Customer harus diisi");
            return false;
        }
        else if (data.kodeBarang == "") {
            alert("Kode Barang harus diisi");
            return false;
        }
        else if (data.tanggalBeli == "") {
            alert("Tanggal Beli harus diisi");
            return false;
        }
        else {
            return true;
        }
    };
    ServiceRequestComponent.prototype.cekCustomer = function (kodeCustomer) {
        var _this = this;
        return this.service.getCustomer(kodeCustomer).then(function (data) {
            if (data.length > 0) {
                return true;
            }
            else {
                alert("Customer tidak ada dalam database");
                return false;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            return false;
        });
    };
    ServiceRequestComponent.prototype.cekBarang = function (kodeBarang) {
        var _this = this;
        return this.service.getBarangByKode(kodeBarang).then(function (data) {
            if (data.length > 0) {
                return true;
            }
            else {
                alert("kode barang ini tidak ada dalam database");
                return false;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            return false;
        });
    };
    // event event simpan or edit
    ServiceRequestComponent.prototype.save = function () {
        var _this = this;
        if (this.validation(this.data)) {
            //  validasi cek customer di database
            this.cekCustomer(this.data.kodeCustomer).then(function (result) {
                if (result) {
                    // validasi cek barang di database
                    _this.cekBarang(_this.data.kodeProduk).then(function (result) {
                        if (result) {
                            if (_this.data.kodeService == "") {
                                // insert ke database
                                _this.service.serviceInsert(_this.data).then(function (data) {
                                    _this.data['kodeService'] = data.kode_service;
                                    var bassPusat = JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat;
                                    if (_this.gloKodeBass == bassPusat && _this.gloKodeBass !== _this.data.assignToBass) {
                                        // masuk sini
                                        _this.sendMail();
                                    }
                                    alert(data.kode_service + "\n Berhasil Disimpan");
                                }, function (err) {
                                    if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                        alert("Your Token has expired, please login again !");
                                        sessionStorage.clear();
                                        _this.router.navigate(['/login']);
                                    }
                                    else {
                                        alert(err._body.data);
                                    }
                                });
                            }
                            else {
                                // update ke database
                                console.log(_this.data);
                                _this.service.serviceUpdate(_this.data).then(function (data) {
                                    var bassPusat = JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat;
                                    if (_this.gloKodeBass == bassPusat && _this.gloKodeBass !== _this.data.assignToBass) {
                                        // masuk sini
                                        _this.sendMail();
                                    }
                                    alert(data.kode_service + "\n Berhasil Diubah");
                                }, function (err) {
                                    if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                                        alert("Your Token has expired, please login again !");
                                        sessionStorage.clear();
                                        _this.router.navigate(['/login']);
                                    }
                                    else {
                                        alert(err._body.data);
                                    }
                                });
                            }
                        }
                    });
                }
            });
        }
    };
    // sendmail after save
    ServiceRequestComponent.prototype.sendMail = function () {
        var _this = this;
        var body = "Hello  <br><br>" +
            "A Service Request  has been routed to you with detail below <br>" +
