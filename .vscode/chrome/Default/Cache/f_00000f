        this.status_brg = ['Warranty', 'NonWarranty', 'ServiceWarranty'];
        // variable biaya2
        this.hargaTotalSukuCadang = 0;
        this.biayaService = 0;
        this.subTotal = 0;
        this.ppn = 0;
        this.totalBiayaService = 0;
        this.biayaTransportasi = [{ KODE_TRANS: "", JARAK: "", BIAYA_TRANSPORTASI: 0 }];
        // tombol reject
        this.display = false;
        this.rejectComent = "";
    }
    FinishingServiceRequestComponent.prototype.ngOnInit = function () {
        var _this = this;
        this.gloKodeBass = JSON.parse(sessionStorage.getItem('mAuth')).Kode_Bass;
        this.gloNamaBass = JSON.parse(sessionStorage.getItem('mBass')).Nama_Bass;
        this.gloUsername = JSON.parse(sessionStorage.getItem('mAuth')).Kode_Karyawan;
        // cek hak akses
        this.hakAkses = JSON.parse(sessionStorage.getItem('mRole')).filter(function (data) { return data.App_Code == _this.appCode; })[0];
        if (this.hakAkses.Akses) {
            // validasi tombol insert dan edit
            if (this.hakAkses.Insert) {
                this.flagButtonInsert = true;
                this.flagButtonClose = true;
                this.flagButtonReject = true;
                this.flagButtonReOpen = true;
            }
            if (this.hakAkses.Edit) {
                this.flagEdit = true;
            }
            this.newData();
            if (this.actroute.snapshot.params['no_service']) {
                var kodeService = this.actroute.snapshot.params['no_service'];
                this.service.getServiceList(kodeService, this.gloKodeBass, '', '', 'ServiceRequest', '').then(function (data) {
                    // console.log('ambil data by param: ', data)
                    var event = { data: data[0] };
                    _this.onRowSelectedLookupService(event);
                }, function (err) {
                    console.log(err);
                });
            }
            ;
        }
        else {
            alert('Anda tidak berhak mengakses halaman ini!');
            this.router.navigate(['/pages/home']);
        }
    };
    FinishingServiceRequestComponent.prototype.newData = function () {
        // console.log("load new data")
        this.data = {
            kodeService: "",
            // tanggal: this.today,
            // jamMasuk: this.today.getHours() + ":" + this.today.getMinutes(),
            tanggal: "",
            jamMasuk: "",
            kodeCustomer: "",
            namaCustomer: "",
            alamatCustomer: "",
            telpCustomer: "",
            hpCustomer: "",
            kotaCustomer: "",
            zonaCustomer: "",
            kodeProduk: "",
            namaProduk: "",
            merk: "",
            jenis: "",
            nomorSeri: "",
            tanggalBeli: "",
            kodeBass: this.gloKodeBass,
            jenisService: "Internal",
            statusProduk: this.status_brg[0],
            kodePengaduan: "",
            kelengkapan: "",
            catatan: "",
            biaya: "",
            status: "New",
            inputtedby: this.gloUsername,
            InputtedByBass: this.gloKodeBass,
            inputtedDate: this.today,
            nomorNota: "",
            assignToBass: "",
            alamatAssignToBass: "",
            kotaAssignToBass: "",
            emailAssignToBass: "",
            // property data detail {kodeSparepart, namaSparepart, noInvoice, harga, qty}
            dataDetail: [],
            // tambah propertu keperluan finishing service
            tanggalSelesai: "",
            tanggalKembali: "",
            kodeTeknisi: "",
            diambilOleh: "",
            penyebab: "",
            perbaikan: "",
            kodeInvoice: ""
        };
        this.biayaTransport();
    };
    // fungsi enable disable detail
    FinishingServiceRequestComponent.prototype.Disable_Enable_Detail = function (flag, data) {
        var _this = this;
        this.hakAkses = JSON.parse(sessionStorage.getItem('mRole')).filter(function (data) { return data.App_Code == _this.appCode; })[0];
        if (!this.hakAkses.Insert) {
            flag = false;
        }
        this.flagTanggalSelesaiTextbox = flag;
        this.flagTanggalKembaliTextbox = flag;
        this.flagDiambilOlehCustomerTextbox = flag;
        this.flagTeknisi = flag;
        this.flagPerbaikanCombo = flag;
        this.flagPenyebabCombo = flag;
        if (flag == false) {
            this.flagAddSparepartButton = flag;
            this.flagTanggalSelesaiTextbox = flag;
            this.flagTanggalKembaliTextbox = flag;
            this.flagTeknisi = flag;
        }
        else {
            this.flagRemoveSparepartButton = flag;
            this.flagTanggalSelesaiTextbox = flag;
            this.flagTanggalKembaliTextbox = flag;
            this.flagTeknisi = flag;
        }
        this.flagBiayaTransportCombo = flag;
        this.flagSparepartsQty = flag;
        this.flagButtonInsert = flag;
        // Jika service tersebut bukan dibuat oleh bass pusat maka tidak boleh direject
        if (this.data.Inpuuted_By_Bass == JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat && this.data.kodeBass == JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat) {
            this.flagButtonReject = flag;
        }
        else {
            this.flagButtonReject = false;
        }
        this.flagButtonClose = flag;
        this.flagNomorInvoiceButton = flag;
        this.flagNomorSeriTextbox = flag;
        this.flagTanggalBeliTextbox = flag;
        this.flagAddSparepartButton = flag;
        this.flagPengaduanCombo = flag;
        this.flagNomorNotaTexboxt = flag;
        this.flagStatusBarang = flag;
    };
    //event event service list
    FinishingServiceRequestComponent.prototype.onRowSelectedLookupService = function (event) {
        // console.log(event)
        var _this = this;
        // console.log("on row selectd lookup service : " + JSON.stringify(event.data))
        // Redirect if central bass want to edit rejected service to finishing service all
        if (this.gloKodeBass == JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat
            && event.data.STATUS == "Rejected") {
            this.router.navigate(['pages/service/finishingservicerequestall']);
        }
        // label rejected nilainya false
        this.flagRejected = false;
        if (JSON.parse(sessionStorage.getItem('mAuth')).Type == "Cabang") {
            // panggil fungsi disable enable detail nilain false
            this.Disable_Enable_Detail(false, this.data);
        }
        else if (event.data.STATUS == "Rejected") {
            // panggil fungsi disable enable detail nilai false
            // reject2an nilannya true
            this.Disable_Enable_Detail(false, this.data);
            this.flagRejected = true;
        }
        else if (event.data.STATUS == "Closed" && event.data.Rejection_Comment !== "") {
            // panggil fungsi disable enable detail nilain false
            // reject2an nilannya true
            this.Disable_Enable_Detail(false, this.data);
            this.flagRejected = true;
        }
        else {
            if (event.data.STATUS == "Closed" || event.data.STATUS == "Done") {
                // jika ada di table review dan statusnya 0 maka diaktifkan lagi untuk pengeditannya
                // panggil review GET_REVIEW_CLAIM_SERVICE
                this.getReviewClaimService(event.data.KODE_SERVICE);
            }
            else {
                if (event.data.KODE_CLAIM == "" || event.data.KODE_CLAIM == undefined) {
                    // disable enable detail true
                    this.Disable_Enable_Detail(true, this.data);
                }
            }
        }
        // Visible of close and reopen button
        this.flagButtonReOpen = false;
        this.flagButtonClose = false;
        if (event.data.STATUS != "Closed" && (event.data.KODE_CLAIM == "" || event.data.KODE_CLAIM == undefined)) {
            // close button = true
            this.flagButtonClose = true;
        }
        else {
            // if status review not valid = true
            // maka close button true
            if (this.statusReviewNotValid) {
                this.flagButtonClose = true;
            }
        }
        if (event.data.STATUS == "New" && this.gloKodeBass == JSON.parse(sessionStorage.getItem('mParameter')).Bass_Pusat) {
            // update status jadi opened
            this.service.updatestatus(event.data.KODE_SERVICE, 'Opened').then(function (data) {
                // console.log("Update status : " + event.data)
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
        // property data detail {kodeSparepart, namaSparepart, noInvoice, harga, qty}
        this.data.dataDetail = [];
        this.pengaduan(event.data.KODE_PRODUK);
        this.penyebab(event.data.KODE_PRODUK, event.data.KODE_PENGADUAN);
        this.getDetailServiceRequest(event.data.KODE_SERVICE);
        this.getDetailServiceRequestReceived(event.data.KODE_SERVICE);
        this.data['kodeService'] = event.data.KODE_SERVICE;
        this.data['tanggal'] = new Date(event.data.TANGGAL);
        this.data['jamMasuk'] = event.data.JAM_MASUK;
        this.data['kodeCustomer'] = event.data.KODE_CUSTOMER;
        this.data['namaCustomer'] = event.data.NAMA_CUSTOMER;
        this.data['alamatCustomer'] = event.data.ALAMAT_CUSTOMER;
        this.data['telpCustomer'] = event.data.TELP_CUSTOMER;
        this.data['hpCustomer'] = event.data.HP_CUSTOMER;
        this.data['kotaCustomer'] = event.data.KOTA_CUSTOMER;
        this.data['zonaCustomer'] = event.data.NAMA_ZONA;
        this.data['kodeProduk'] = event.data.KODE_PRODUK;
        this.data['namaProduk'] = event.data.NM_BRG;
        this.data['merk'] = event.data.MERK;
        this.data['jenis'] = event.data.JNS_BRG;
        this.data['nomorSeri'] = event.data.NOMOR_SERI;
        this.data['tanggalBeli'] = new Date(event.data.TANGGAL_BELI);
        this.data['kodeBass'] = event.data.KODE_BASS;
        this.data['jenisService'] = event.data.JENIS_SERVICE;
        this.data['statusProduk'] = event.data.STATUS_PRODUK;
        this.data['kodePengaduan'] = event.data.KODE_PENGADUAN;
        this.data['kelengkapan'] = event.data.KELENGKAPAN;
        this.data['catatan'] = event.data.CATATAN;
        this.data['biaya'] = event.data.BIAYA;
        this.data['status'] = event.data.STATUS;
        this.data['nomorNota'] = event.data.NOMOR_NOTA;
        // tambah propertu keperluan finishing service
        if (!event.data.TANGGAL_SELESAI) {
            this.data['tanggalSelesai'] = "";
        }
        else {
            this.data['tanggalSelesai'] = new Date(event.data.TANGGAL_SELESAI);
        }
        if (!event.data.TANGGAL_KEMBALI) {
            this.data['tanggalKembali'] = "";
        }
        else {
            this.data['tanggalKembali'] = new Date(event.data.TANGGAL_KEMBALI);
        }
        if (!event.data.KODE_TEKNISI) {
            this.data['kodeTeknisi'] = "";
        }
        else {
            this.data['kodeTeknisi'] = event.data.KODE_TEKNISI;
        }
        if (!event.data.DIAMBIL_OLEH) {
            this.data['diambilOleh'] = "";
        }
        else {
            this.data['diambilOleh'] = event.data.DIAMBIL_OLEH;
        }
        this.calculate();
        this.flaghidenMain = false;
    };
    FinishingServiceRequestComponent.prototype.onCancelLookupService = function (flag) {
        this.flaghidenMain = flag;
    };
    FinishingServiceRequestComponent.prototype.onCancelLookupTeknisi = function (flag) {
        this.flaghidenMain = flag;
    };
    FinishingServiceRequestComponent.prototype.onCancelLookupPart = function (flag) {
        this.flaghidenMain = flag;
    };
    FinishingServiceRequestComponent.prototype.onCancelLookupFaktur = function (flag) {
        this.flaghidenMain = flag;
    };
    FinishingServiceRequestComponent.prototype.backToMain = function () {
        this.router.navigate(['/home']);
    };
    // ambil data teknisi
    FinishingServiceRequestComponent.prototype.onRowSelectedLookupTeknisi = function (event) {
        this.data['kodeTeknisi'] = event.data.KODE_TEKNISI;
        this.flaghidenMain = false;
    };
    // ambil data part
    FinishingServiceRequestComponent.prototype.onRowSelectedLookupPart = function (data) {
        // console.log("onRowSelectedLookUpPart : " + data)
        for (var i = 0; i < data.length; i++) {
            var flagEqual = false;
            for (var ii = 0; ii < this.data.dataDetail.length; ii++) {
                // stok2an blom beres
                this.getStokInvoiceSelectByKodePartAndInvoice(this.gloKodeBass, this.data.dataDetail[ii].KD_SPAREPART, this.data.dataDetail[ii].NO_INVOICE);
                if (data[i].KD_SPAREPART == this.data.dataDetail[ii].KD_SPAREPART) {
                    flagEqual = true;
                    break;
                }
            }
            if (!flagEqual) {
                this.data.dataDetail.push(data[i]);
            }
        }
        this.flaghidenMain = false;
        this.calculate();
    };
    FinishingServiceRequestComponent.prototype.onRowSelectedFaktur = function (data) {
        // console.log("onRowSelectedFaktur : " + data)
        for (var i = 0; i < data.length; i++) {
            var flagEqual = false;
            for (var ii = 0; ii < this.data.dataDetail.length; ii++) {
                // stok2an blom beres
                this.getStokInvoiceSelectByKodePartAndInvoice(this.gloKodeBass, this.data.dataDetail[ii].KD_SPAREPART, this.data.dataDetail[ii].NO_INVOICE);
                if (data[i].KD_SPAREPART == this.data.dataDetail[ii].KD_SPAREPART) {
                    flagEqual = true;
                    break;
                }
            }
            if (!flagEqual) {
                this.data.dataDetail.push(data[i]);
            }
        }
        this.flaghidenMain = false;
        this.calculate();
        alert(this.flagButtonInsert);
    };
    FinishingServiceRequestComponent.prototype.onChangeComboBiayaTransport = function () {
        this.calculate();
    };
    FinishingServiceRequestComponent.prototype.onChangeQtySparepart = function () {
        this.calculate();
    };
    FinishingServiceRequestComponent.prototype.onDeletedDetail = function (index) {
        this.data.dataDetail.splice(index, 1);
        this.calculate();
    };
    // event onchange combobox status barang
    FinishingServiceRequestComponent.prototype.onChangeStatusBarang = function (status) {
        this.ongkosService(status, this.data.kodeBarang, this.data.merk, this.data.jenis);
    };
    // event onchange combobox penyebab
    FinishingServiceRequestComponent.prototype.onChangeComboPenyebab = function (penyebab) {
        this.perbaikan(this.data.kodeProduk, this.data.kodePengaduan, penyebab);
    };
    // event onchange combobox pengaduan
    FinishingServiceRequestComponent.prototype.onChangeComboPengaduan = function (pengaduan) {
        this.penyebab(this.data.kodeProduk, pengaduan);
    };
    // fungsi buat panggil api
    FinishingServiceRequestComponent.prototype.getDetailServiceRequest = function (kodeService) {
        var _this = this;
        this.service.getDetailServiceRequest(kodeService).then(function (data) {
            _this.data.dataDetail = data;
            // console.log("get detail : " + this.data.dataDetail)
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestComponent.prototype.getReviewClaimService = function (kodeService) {
        var _this = this;
        this.service.getReviewClaimService(kodeService).then(function (data) {
            // console.log("get review claim service : " + data)
            /*
                belum berfungsi
                jika row nya > 0
                jika row = 0 maka status review not valid
                jika status review not valid = true mka disable enable true dan sebaliknya
            */
            if (data.length > 0) {
                if (data[0].ISVALID = 0) {
                    _this.statusReviewNotValid = true;
                }
            }
            if (_this.statusReviewNotValid) {
                _this.Disable_Enable_Detail(true, _this.data);
            }
            else {
                _this.Disable_Enable_Detail(false, _this.data);
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestComponent.prototype.getStokInvoiceSelectByKodePartAndInvoice = function (kodeBass, kodePart, kodeInvoice) {
        var _this = this;
        this.service.getStokInvoiceSelectByKodePartAndInvoice(kodeBass, kodePart, kodeInvoice).then(function (data) {
            // console.log("stok :" + data)
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestComponent.prototype.getDetailServiceRequestReceived = function (kodeService) {
        var _this = this;
        this.service.getDetailServiceRequestReceived(kodeService).then(function (data) {
            _this.detailServiceRequestReceivedList = data;
            // console.log(this.detailServiceRequestReceivedList)
            if (_this.detailServiceRequestReceivedList.length > 0) {
                _this.LblNotificationPO = false;
            }
            else {
                _this.LblNotificationPO = true;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestComponent.prototype.ongkosService = function (status, kodeBarang, merk, jenis) {
        var _this = this;
        if (status == "NonWarranty") {
            this.servicerequest.getHarga(kodeBarang, merk, jenis).then(function (data) {
                _this.data['biaya'] = data[0].ongkos;
                _this.calculate();
            }, function (err) {
                if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                    alert("Your Token has expired, please login again !");
                    sessionStorage.clear();
                    _this.router.navigate(['/login']);
                }
                else {
                    alert(err._body.data);
                }
            });
        }
        else {
            this.data['biaya'] = 0;
            this.calculate();
        }
    };
    // ppn
    FinishingServiceRequestComponent.prototype.hitungPPN = function (hargaSukuCadang, hargaService, hargaTransport) {
        var _this = this;
        // console.log("ambil biaya ppn")
        this.service.calculatePPN(hargaSukuCadang, hargaService, hargaTransport).then(function (data) {
            _this.ppn = data[0].ppn;
            // console.log("ini ppn : " + this.ppn)
            // total biaya service
            _this.totalBiayaService = hargaSukuCadang + hargaService + hargaTransport + _this.ppn;
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            // console.log(err)
        });
    };
    // biaya transport
    FinishingServiceRequestComponent.prototype.biayaTransport = function () {
        var _this = this;
        // console.log("fect biaya transport")
        this.service.getBiayaTransportasi().then(function (data) {
            _this.biayaTransportList = data;
            _this.biayaTransportasi = _this.biayaTransportList[0];
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
            // console.log(err)
        });
    };
    FinishingServiceRequestComponent.prototype.pengaduan = function (kodeBarang) {
        var _this = this;
        // console.log("fect data pengaduan")
        this.servicerequest.getKerusakan(kodeBarang).then(function (data) {
            _this.kerusakanList = data;
            if (_this.data['kodePengaduan'] == "") {
                _this.data['kodePengaduan'] = _this.kerusakanList[0].Nama_Kerusakan;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestComponent.prototype.perbaikan = function (kodeBarang, namaKerusakan, namaPenyebab) {
        var _this = this;
        // console.log("fect data perbaikan")
        this.service.getPerbaikan(kodeBarang, namaKerusakan, namaPenyebab).then(function (data) {
            _this.perbaikanList = data;
            // console.log(this.perbaikanList)
            if (_this.perbaikanList.length > 0) {
                _this.data['perbaikan'] = _this.perbaikanList[0].NAMA_PERBAIKAN;
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    FinishingServiceRequestComponent.prototype.penyebab = function (kodeBarang, namaKerusakan) {
        var _this = this;
        // console.log("fect data penyebab")
        // console.log(kodeBarang + " " + namaKerusakan)
        this.service.getPenyebab(kodeBarang, namaKerusakan).then(function (data) {
            _this.penyebabList = data;
            if (_this.penyebabList.length > 0) {
                _this.data['penyebab'] = _this.penyebabList[0].Nama_Penyebab;
                _this.perbaikan(kodeBarang, namaKerusakan, _this.penyebabList[0].Nama_Penyebab);
            }
        }, function (err) {
            if (err._body.data.indexOf("TokenExpiredError") == 1 && err.status == 500) {
                alert("Your Token has expired, please login again !");
                sessionStorage.clear();
                _this.router.navigate(['/login']);
            }
            else {
                alert(err._body.data);
            }
        });
    };
    // fungsi lain lain
    FinishingServiceRequestComponent.prototype.onHiddenMain = function (flag) {
        this.flaghidenMain = flag;
    };
    // fungsi menghitung biaya service
    FinishingServiceRequestComponent.prototype.calculate = function () {
        var totalSukuCadang = 0;
        // hitung total suku cadang
        if (this.data.dataDetail.length > 0) {
            for (var i = 0; i < this.data.dataDetail.length; i++) {
                totalSukuCadang += Number(this.data.dataDetail[i].QTY) * Number(this.data.dataDetail[i].HARGA);
            }
        }
        // cek garansi jika status waranty makan total suku cadang dan biaya service = 0
        if (this.data.statusProduk == "Warranty") {
            this.hargaTotalSukuCadang = 0;
            this.biayaService = 0;
        }
        else {
            this.hargaTotalSukuCadang = totalSukuCadang;
            this.biayaService = this.data.biaya;
        }
        this.subTotal = this.hargaTotalSukuCadang + this.biayaService + this.biayaTransportasi.BIAYA_TRANSPORTASI;
        // // hitung ppn
        this.hitungPPN(this.hargaTotalSukuCadang, this.biayaService, this.biayaTransportasi.BIAYA_TRANSPORTASI);
    };
    // crud button
    FinishingServiceRequestComponent.prototype.save = function () {
        var _this = this;
        // validasi
        if (this.validation(this.data)) {
            this.cekTeknisi(this.data.kodeTeknisi).then(function (result) {
                if (result) {
                    _this.data.status = "Done";
