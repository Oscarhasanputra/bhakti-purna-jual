        this.filterPart['kodeBarang'] = kodeBarang;
        this.filterPart['kodeInvoice'] = kodeInvoice;
        this.filterPart['jenisService'] = jenisService;
        this.filterPart['kodeFinishing'] = kodeFinishing;
