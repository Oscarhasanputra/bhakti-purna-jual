{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "outDir": "../out-tsc/app",
      "baseUrl": "./",
      "paths": {
        "@angular/*": [
          "../node_modules/@angular/*"
        ],
        "@nebular/*": [
          "../node_modules/@nebular/*"
        ]
      }
    },
    "exclude": [
      "test.ts",
      "**/*.spec.ts"
    ],
    "include": [
      "../src/*.ts",
      "../src/**/*.ts",
    ],
  }